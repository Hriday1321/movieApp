<%- include('./partials/header.ejs') %>
    <title>
        Home Page
    </title>
    <link rel="stylesheet" href="/home.css">
</head>
<body>

<h1>HOMEE</h1>



<div id="container">
    <div id="nav">
        <div class="left">
            <form action="/title" method="GET">
                <input type="text" name="title" placeholder="Enter Title term">
                <button>Submit</button>
            </form>
        </div>
        <div class="right">
            <form action="/search" method="GET">
                <input type="text" name='search' placeholder="Enter Search term">
                <button>Submit</button>
            </form>
        </div>
    </div>
    <% data["Search"].forEach(function(data){ %>
        <div class="f">
                <div class="lfl">
                    <img src="<%= data["Poster"] %>">
                </div>
                <div class="rfl">
                    <div class="title">
                        <%= data["Title"] %>
                    </div>
                    <div class="year">
                        <p>
                            Year Released: <%= data["Year"] %>
                        </p>
                    </div>
                    <div class="type">
                        <p>
                            Type: <%= data["Type"] %>
                        </p>
                    </div>
                </div>
        </div>
    <% }) %>
</div>

<%- include('./partials/footer.ejs') %>